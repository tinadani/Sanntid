from threading import Thread 

i=0

def threadOne():
	global i
	for j in range(1000000):
		i+=1

def threadTwo():
	global i
	for j in range(1000000):
		i-=1

def main():
	thread_1=Thread(target=threadOne, args=(),)
	thread_1.start()
	thread_1.join()
	thread_2=Thread(target=threadTwo, args=(),)
	thread_2.start()
	thread_2.join()
	print('i=',i)

main()
